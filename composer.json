{
    "name": "netpromotion/symfony-up",
    "description": "Up your Symfony App!",
    "homepage": "https://netpromotion.github.io/symfony-up/",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "knap@netpromotion.cz"
        },
        {
            "name": "Filip Halaxa",
            "email": "halaxa@netpromotion.cz"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^3.4",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/routing": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "doctrine/common": "*",
        "phpunit/phpunit": "^4.8",
        "symfony/phpunit-bridge": "^3.4",
        "sensio/framework-extra-bundle": "^3.0"
    },
    "suggest": {
        "doctrine/common": "Registers an autoloading for annotations",
        "symfony/asset": "Manages URL generation and versioning of web assets (uncomment `framework.assets` part of `app/config/config.yml`)"
    },
    "conflict": {
        "doctrine/common": "<2.7 || >=3"
    },
    "bin": ["bin/symfony-up"],
    "autoload": {
        "psr-4": {
            "Netpromotion\\SymfonyUp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netpromotion\\SymfonyUp\\Test\\": "tests"
        }
    }
}
