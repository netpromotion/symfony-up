{
    "name": "netpromotion/symfony-up",
    "description": "Up your Symfony App!",
    "homepage": "https://netpromotion.github.io/symfony-up/",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "knap@netpromotion.cz"
        },
        {
            "name": "Filip Halaxa",
            "email": "halaxa@netpromotion.cz"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/config": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/routing": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "symfony/dotenv": "*",
        "symfony/phpunit-bridge": "^4.1",
        "sensio/framework-extra-bundle": "^5.1"
    },
    "suggest": {
        "symfony/asset": "Manages URL generation and versioning of web assets (uncomment `framework.assets` part of `app/config/config.yml`)",
        "symfony/dotenv": "Registers environment variables from a .env file"
    },
    "conflict": {
        "symfony/dotenv": "<4.1 || >=5"
    },
    "bin": ["bin/symfony-up"],
    "autoload": {
        "psr-4": {
            "Netpromotion\\SymfonyUp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netpromotion\\SymfonyUp\\Test\\": "tests"
        }
    }
}
