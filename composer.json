{
    "name": "netpromotion/symfony-up",
    "description": "Package for rapid simplification of Symfony applications and their development",
    "homepage": "https://github.com/netpromotion/symfony-up",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "knap@netpromotion.cz"
        },
        {
            "name": "Filip Halaxa",
            "email": "halaxa@netpromotion.cz"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^3.2",
        "symfony/config": "^3.2",
        "symfony/console": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/http-kernel": "^3.2",
        "symfony/routing": "^3.2"
    },
    "require-dev": {
        "doctrine/common": "*",
        "phpunit/phpunit": "^4.8",
        "symfony/browser-kit": "^3.2",
        "symfony/phpunit-bridge": "^3.2",
        "sensio/framework-extra-bundle": "^3.0"
    },
    "conflict": {
        "doctrine/common": "<2.7 || >=3"
    },
    "bin": ["bin/symfony-up"],
    "autoload": {
        "psr-4": {
            "Netpromotion\\SymfonyUp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netpromotion\\SymfonyUp\\Test\\": "tests"
        }
    }
}
